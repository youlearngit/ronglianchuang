/* pages/registration/registration.wxss */
/* 提示注册弹窗 */
.enrollBox {
  width: 100%;
  height: 100vh;
  background: #bbbcbf;
  position: relative;
}

.enrollBox_count {
  background-color: #fff;
  width: 80%;
  margin: auto;
  position: absolute;
  top: 30%;
  left: 10%;
  border-radius: 30rpx;
  text-align: center;
}
.enrollBox_btn{
  background: #4181FC;
  box-shadow: 0 3px 13px 0 rgba(85, 148, 255, 0.50);
  border-radius: 45rpx;
  color: #FFFFFF;
  font-size: 34rpx;
  font-weight: 500;
  width: 500rpx;
}
.enrollBox_count1{
  margin-top: 80rpx;
  margin-bottom: 80rpx;
  font-size: 35rpx;
  font-family: SourceHanSansSC-Regular;
color: #3E415C;
letter-spacing: 0;
}
.enrollBox_count2{
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-family: SourceHanSansSC-Regular;
  font-size: 30rpx;
  color: #ACAFC9;
  letter-spacing: 0;
}